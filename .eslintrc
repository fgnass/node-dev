{
  "env": {
    "es6": true
  },
  "extends": [
    "airbnb-base/legacy"
  ],
  "parserOptions": {
    "ecmaVersion": 9
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "consistent-return": 0,
    "eqeqeq": 0,
    "func-names": 0,
    "guard-for-in": 0,
    "global-require": 0,
    "one-var": [2, "never"],
    "padded-blocks": 0,
    "vars-on-top": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-use-before-define": 0,
    "strict": 0
  }
}
